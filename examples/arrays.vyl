Function Main() {
    var array nums = [10, 20, 30, 40, 50];
    
    Print("Element 0: ");
    Print(nums[0]);
    Print("\n");
    
    Print("Element 2: ");
    Print(nums[2]);
    Print("\n");
    
    Print("Sum of first two: ");
    Print(nums[0] + nums[1]);
    Print("\n");
    
    // Modify an element
    nums[1] = 100;
    Print("After nums[1] = 100: ");
    Print(nums[1]);
    Print("\n");
}
