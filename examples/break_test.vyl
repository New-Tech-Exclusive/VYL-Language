//NOTE: break and continue are semi-broken, do not use them yet.

Main() {
    var sum = 0
    for i in 1..10 {
        if (i == 3) {
            continue
        }
        if (i == 8) {
            break
        }
        sum = sum + i
        Print(i)
        Print(" ")
    }
    Print("\nSum: ")
    Print(sum)
    Print("\n")
}
